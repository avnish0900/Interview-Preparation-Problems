int inSequence(int a,int b,int c){
        if(c==0){
            if(a==b){
                return true;
            }
            return false;
        }
        else{
            if((b-a)/c <0){
                return false;
            }
            else{
                if((b-a)% c==0){
                    return true;
                }
                return  false;
            }
        }
    }
